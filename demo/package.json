{
  "name": "cabidela-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules/.cache/wrangler",
    "test": "echo \"Error: no test specified\" && exit 1",
    "env": "eval $(cat .dev.vars) env",
    "build": "npx rollup --bundleConfigAsCjs -c rollup.config.mjs",
    "start": "eval $(cat .dev.vars) npx wrangler pages dev dist --persist-to ~/.wrangler",
    "rollup": "npx rollup --bundleConfigAsCjs -c rollup.config.mjs --watch --no-watch.clearScreen",
    "dev": "npx concurrently --raw --names \"rollup,admin\" --handle-input --default-input-target admin \"npm run rollup\" \"npm start\"",
    "deploy": "make deploy",
    "deploy-production": "ENV=production make deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cloudflare/cabidela": "^0.0.17",
    "@headlessui/tailwindcss": "^0.2.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^12.1.0",
    "@tailwindcss/forms": "^0.5.9",
    "@types/js-yaml": "^4.0.9",
    "@types/postcss-import": "^14.0.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "dayjs": "^1.11.13",
    "fast-glob": "^3.3.2",
    "json5": "^2.2.3",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.2.0",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "react-simple-code-editor": "^0.14.1",
    "react-toastify": "^10.0.5",
    "rollup": "^4.24.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-import-css": "^3.5.6",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "rollup-preserve-directives": "^1.1.2",
    "sass": "^1.79.4",
    "tailwindcss": "^3.4.13",
    "tslib": "^2.7.0",
    "wrangler": "^3.80.1"
  }
